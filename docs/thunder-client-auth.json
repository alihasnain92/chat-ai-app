  "requests": [
    {
      "name": "Register User",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/register",
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "body": {
        "type": "json",
        "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john.doe@example.com\",\n  \"password\": \"SecurePass123\"\n}",
        "form": []
      },
      "tests": [
        {
          "type": "set-env-var",
          "action": "setTo",
          "value": "{{token}}",
          "varName": "token"
        },
        {
          "type": "res-code",
          "action": "equal",
          "value": "201"
        },
        {
          "type": "json-query",
          "action": "equal",
          "value": "json.token",
          "expression": "token"
        }
      ],
      "colId": "ai-chat-auth-collection",
      "created": "2025-11-05T00:00:00.000Z",
      "modified": "2025-11-05T00:00:00.000Z"
    },
