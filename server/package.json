{
  "name": "chat-ai-server",
  "version": "1.0.0",
  "description": "Backend server for the Chat AI App (Express + TypeScript + Prisma + Socket.IO)",
  "main": "dist/index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "prisma db seed",
    "seed:dev": "ts-node prisma/seed.ts",
    "test": "cross-env NODE_ENV=test dotenv_config_path=.env.test npx jest --runInBand",
    "test:watch": "cross-env NODE_ENV=test dotenv_config_path=.env.test npx jest --watch",
    "test:coverage": "cross-env NODE_ENV=test dotenv_config_path=.env.test npx jest --coverage",
    "test:verbose": "cross-env NODE_ENV=test dotenv_config_path=.env.test npx jest --verbose",
    "test:hash": "cross-env NODE_ENV=test node -r dotenv/config ./node_modules/.bin/jest src/utils/hash.test.ts dotenv_config_path=.env.test",
    "test:setup": "node -e \"require('dotenv').config({ path: '.env.test' }); const { execSync } = require('child_process'); execSync('npx prisma db push --skip-generate --accept-data-loss --force-reset', { stdio: 'inherit' });\""
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.15.6",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.24",
    "@types/supertest": "^6.0.3",
    "bcryptjs": "^3.0.3",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.7",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "author": "Your Name",
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "socket.io",
    "chat-app",
    "api",
    "backend"
  ]
}
